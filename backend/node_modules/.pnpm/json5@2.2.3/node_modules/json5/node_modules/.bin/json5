#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/json5@2.2.3/node_modules/json5/lib/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/json5@2.2.3/node_modules/json5/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/json5@2.2.3/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules:/mnt/c/Users/saint/OrdinalScale/base/node_modules:/mnt/c/Users/saint/OrdinalScale/node_modules:/mnt/c/Users/saint/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/saint/OrdinalScale/Base/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/json5@2.2.3/node_modules/json5/lib/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/json5@2.2.3/node_modules/json5/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/json5@2.2.3/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules:/mnt/c/Users/saint/OrdinalScale/base/node_modules:/mnt/c/Users/saint/OrdinalScale/node_modules:/mnt/c/Users/saint/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/saint/OrdinalScale/Base/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
