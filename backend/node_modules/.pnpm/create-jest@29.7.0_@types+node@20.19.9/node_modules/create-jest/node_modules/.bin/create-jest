#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.9/node_modules/create-jest/bin/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.9/node_modules/create-jest/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.9/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules:/mnt/c/Users/saint/OrdinalScale/base/node_modules:/mnt/c/Users/saint/OrdinalScale/node_modules:/mnt/c/Users/saint/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/saint/OrdinalScale/Base/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.9/node_modules/create-jest/bin/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.9/node_modules/create-jest/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.9/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/saint/OrdinalScale/base/backend/node_modules:/mnt/c/Users/saint/OrdinalScale/base/node_modules:/mnt/c/Users/saint/OrdinalScale/node_modules:/mnt/c/Users/saint/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/saint/OrdinalScale/Base/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../bin/create-jest.js" "$@"
fi
